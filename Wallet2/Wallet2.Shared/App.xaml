<Application
    x:Class="Wallet2.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Uno.UI.Demo"
    RequestedTheme="Light">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- Load WinUI resources -->
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls"/>

                <!-- Application's custom styles -->
                <!-- other ResourceDictionaries -->

                <ResourceDictionary Source="CodeBox.xaml" />
                <ResourceDictionary Source="DefaultStyles.xaml" />
                <ResourceDictionary Source="Styles/_Styles.xaml" />
                <ResourceDictionary Source="Icons.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        <!--<Style TargetType="ToggleSwitch"
            BasedOn="{StaticResource MaterialToggleSwitchStyle}" />-->
    </Application.Resources>
</Application>
